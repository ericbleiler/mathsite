<div class="wrapper">

    <article class="main">
        <div id="toplabel">Level 2 Subtraction</div>
        <fieldset id="chooseSign">
            <input type="radio" name="operator" id="subtract" checked="checked" /><label for="subtract">-</label>

        </fieldset>
        <div id="count"></div>
        <div class="popup-container" id="popup1">
            <div class="popup-wrapper">
                <div class="popup-content">
                    <div id="popuptext"></div>
                    <button id="popup1close">Close</button>
