                    <span id="num2" type="text"></span>
                    =
                    <input id="userAnswer" type="text" name="userAnswer" />
            </h1>
            <a href="javascript:void(0)" id="popup1open"></a>
            <button id="submit">Solve</button>
            <button id="reset">Next</button><br />
            <center><input type="submit" id="enter" /></center>

            <input id="number1" type="text" name="num1" style="visibility:hidden;" />
            <input id="number2" type="text" name="num2" style="visibility:hidden;" />
            <input id="user" type="text" name="user" style="visibility:hidden;" />
            <input id="correct" type="text" name="correct" style="visibility:hidden;" />
            <input id="questionNum" type="text" name="questionNum" style="visibility:hidden;" />
            <input id="addLevel" type="text" name="addLevel" value="2" style="visibility:hidden" />


            </form>

